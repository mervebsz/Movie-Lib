            "program": "bin/main.dart"
        },
        
        {
            "name": "Dart: Attach to Process",
            "type": "dart",
            "request": "attach"
        },


        
        {
            "name": "Android 14 (API 34) (emulator)",
            "program": "lib/main.dart",
